# Razorpay Configuration
# Get these from: https://dashboard.razorpay.com/app/keys
RAZORPAY_KEY_ID=rzp_test_xxxxxxxxxxxxx
RAZORPAY_KEY_SECRET=your_key_secret_here

# Merchant Information
MERCHANT_NAME=MediFind
MERCHANT_UPI_ID=yourbusiness@paytm

# Webhook Secret (Optional)
# Get this from: Razorpay Dashboard > Settings > Webhooks
RAZORPAY_WEBHOOK_SECRET=your_webhook_secret_here

# Server Configuration
PORT=3001
NODE_ENV=development

# Firebase Admin (if using Firebase for database updates)
# You can add Firebase service account path here if needed
# FIREBASE_SERVICE_ACCOUNT=./serviceAccountKey.json

